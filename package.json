{
  "name": "concurrently",
  "version": "1.0.0",
  "description": "Allows start both frontend (after added) and backend servers at the same time",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"cd backend && npm run dev\" ",
    "start": "concurrently \"cd backend && npm run start\" ",
    "db-create": "cd backend && npm run db-create",
    "db-drop": "cd backend && npm run db-drop",
    "db-import": "cd backend && npm run db-import"
  },
