{
  "name": "concurrently",
  "version": "1.0.0",
  "description": "Allows start both frontend (after added) and backend servers at the same time",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"cd backend && npm run dev\" ",
    "start": "concurrently \"cd backend && npm run start\" ",
    "db-create": "\"node -e require('./backend/database/migration').migrate()\"",
    "db-drop": "\"node -e require('./backend/database/scripts/execSql').execSql('./backend/database/scripts/dropTables.sql')\""
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^7.4.0"
  }
}
